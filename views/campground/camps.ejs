<%- include("../partials/header") -%>
<div class="container">
<header class="jumbotron">
	<div class="container">
		<h1>
	Capms page
   </h1>
	<p>
		All mindblowing campgrounds are here....
	</p>
   <a href="camps/new" class="btn btn-primary">Add new one</a>

	</div>
	
</header>

<div class="row" style="display:flex;flex-wrap:wrap;">
	<% camps.forEach(function(camps){%>
	<div class="col-md-3 col-sm-6">
		
	<div class="img-thumbnail">
		<img src="<%=camps.img%>" class="img-fluid" >
		<h4>
	<div class="caption" style="text-align:center">
		<br>
		<%= camps.name%><div><br>
		<a href="/camps/<%= camps._id%>" class="btn btn-primary">MORE INFO</a>
		</div>
		
			</div>	
	</h4>
	</div>
		
	</div>
<%});%>
</div>

</div>
<%- include("../partials/footer") -%>